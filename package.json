{
  "name": "telltide-server",
  "version": "1.0.0",
  "description": "TellTide - Meta-event detection and webhook notification system for ERC20 and ERC4626 vault events",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm indexer\" \"pnpm worker\" \"pnpm api\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "indexer": "tsx src/indexer/index.ts",
    "indexer:ethereum": "tsx src/indexer/index.ts ethereum",
    "indexer:base": "tsx src/indexer/index.ts base",
    "api": "tsx src/api/index.ts",
    "worker": "tsx src/worker/index.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:migrate:chain": "tsx src/db/migrate-chain.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:insert-subs": "tsx src/db/insert-subscriptions.ts",
    "db:clean": "tsx src/db/clean-db.ts",
    "db:clean-subs": "tsx src/db/clean-subscriptions.ts",
    "db:debug": "tsx src/db/debug-detection.ts"
  },
  "keywords": [
    "blockchain",
    "sqd",
    "webhooks",
    "erc20",
    "erc4626",
    "defi",
    "telltide"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-codec": "^0.3.0",
    "@subsquid/pipes": "0.1.0-beta.6",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
